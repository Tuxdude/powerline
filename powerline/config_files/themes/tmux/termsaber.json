    "segments": {
        "left": [
            {
                "name": "virtualenv"
            },
            {
                "module": "powerline.segments.tuxy",
                "name": "sandbox_id"
            },
            {
                "module": "powerline.segments.tuxy",
                "name": "sandbox_perforce_branch_name"
            },
            {
                "module": "powerline.segments.tuxy",
                "name": "sandbox_flavor"
            }
            ],
        "right": [
            {
                "name": "uptime",
                "priority": 50
            },
            {
                "name": "user"
            },
            {
                "name": "hostname"
            }
        ]
    }
}
