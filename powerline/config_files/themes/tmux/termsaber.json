{
    "default_module": "powerline.segments.common",
    "segment_data": {
        "uptime": {
            "before": "⇑ "
        },
        "external_ip": {
            "before": "ⓦ  "
        },
        "date": {
            "before": "⌚ "
        },
        "email_imap_alert": {
            "before": "✉ ",
            "args": {
                "username": "",
                "password": ""
            }
        },
        "user" : {
                "before": " "
        },
        "hostname": {
            "args": {
                "only_if_ssh": false
            }
        },
        "virtualenv": {
            "before": "ⓔ  "
        },
        "branch": {
            "before": " "
        },
        "sandbox_perforce_branch_name": {
            "before": " "
        }
